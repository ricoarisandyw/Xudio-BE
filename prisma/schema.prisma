generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  username String? @db.VarChar
  password String? @db.VarChar
  id       Int     @id @default(autoincrement())
}

model user_detail {
  iduser     Int?
  email      String? @db.VarChar
  image      String? @db.VarChar
  name       String? @db.VarChar
  nop        String? @db.VarChar
  role       String? @db.VarChar
  created_at String? @db.VarChar
  updated_at String? @db.VarChar
  id         Int     @id @default(autoincrement())
}

model course {
  id          Int       @id @default(autoincrement())
  name        String    @db.VarChar
  description String?   @db.VarChar
  begindate   String?   @db.VarChar
  duedate     String?   @db.VarChar
  createdat   DateTime  @db.Date
  updatedat   DateTime? @default(now()) @db.Date
  companyid   Int?
}

model user_in_course {
  id          Int       @id @default(autoincrement())
  idcourse    Int
  iduser      Int
  score       Int?
  startcourse DateTime? @db.Date
  endcourse   DateTime? @db.Date
}

model room {
  id         Int     @id @default(autoincrement())
  name       String? @db.VarChar
  created_at String? @db.VarChar
  status     String? @db.VarChar
  filled     Int?
  capacity   Int?
}

model user_in_room {
  id      Int  @id @default(autoincrement())
  id_user Int?
  id_room Int?
}

model video {
  id   Int     @id @default(autoincrement())
  link String? @db.VarChar
}
