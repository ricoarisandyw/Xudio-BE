generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  username String? @db.VarChar(100)
  password String? @db.VarChar(100)
  id       Int     @id @default(autoincrement())
}

model user_detail {
  idUser     Int?
  email      String? @db.VarChar(100)
  image      String? @db.VarChar(100)
  name       String? @db.VarChar(100)
  nop        String? @db.VarChar(100)
  role       String? @db.VarChar(100)
  created_at String? @db.VarChar(100)
  updated_at String? @db.VarChar(100)
  id         Int     @id @default(autoincrement())
}

model course {
  id          Int       @id @default(autoincrement())
  name        String    @db.VarChar(100)
  description String?   @db.VarChar(100)
  begindate   String?   @db.VarChar(100)
  duedate     String?   @db.VarChar(100)
  createdat   DateTime  @db.Date
  updatedat   DateTime? @default(now()) @db.Date
  companyid   Int?
}

model user_in_course {
  id          Int       @id @default(autoincrement())
  idcourse    Int
  iduser      Int
  score       Int?
  startcourse DateTime? @db.Date
  endcourse   DateTime? @db.Date
}

model room {
  id         Int     @id @default(autoincrement())
  name       String? @db.VarChar(100)
  createdAt String? @db.VarChar(100)
  status     String? @db.VarChar(100)
  filled     Int?
  capacity   Int?
}

model user_in_room {
  id      Int  @id @default(autoincrement())
  idUser Int?
  idRoom Int?
}

model video {
  id   Int     @id @default(autoincrement())
  link String? @db.VarChar(100)
}
