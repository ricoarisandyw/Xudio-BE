generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model user {
  username String?
  password String?
  id       Int     @id @default(autoincrement())
}

model user_detail {
  idUser     Int?
  email      String?
  image      String?
  name       String?
  nop        String?
  role       String?
  createdAt String?
  updatedAt String?
  id         Int     @id @default(autoincrement())
}

model course {
  id          Int       @id @default(autoincrement())
  name        String
  description String?
  begindate   String?
  duedate     String?
  createdat   DateTime
  updatedat   DateTime? @default(now())
  companyId   Int?
}

model user_in_course {
  id          Int       @id @default(autoincrement())
  idCourse    Int
  idUser      Int
  score       Int?
  startcourse DateTime?
  endcourse   DateTime?
}

model room {
  id        Int     @id @default(autoincrement())
  name      String?
  createdAt String?
  status    String?
  filled    Int?
  capacity  Int?
}

model user_in_room {
  id     Int  @id @default(autoincrement())
  idUser Int?
  idRoom Int?
}

model video {
  id   Int     @id @default(autoincrement())
  link String?
}

model adminRoom {
  id   Int     @id @default(autoincrement())
  idRoom Int?
}

model roomInAdmin {
  id   Int     @id @default(autoincrement())
  idRoom Int?
  idAdmin Int?
}